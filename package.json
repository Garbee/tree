{
  "name": "@garbee/tree",
  "version": "0.0.0",
  "description": "Web component providing a tree system",
  "keywords": [
    "lit",
    "lit-element",
    "tree",
    "web-components"
  ],
  "license": "MIT",
  "author": "Jonathan Garbee <jonathan@garbee.me>",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "analyze": "cem analyze --litelement --globs \"src/**/*.ts\"",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "run-p clean:cem clean:dist",
    "clean:cem": "del custom-elements.json",
    "clean:dist": "del dist",
    "clean:docs": "del man",
    "docs:gen": "eleventy --config=.eleventy.js",
    "docs:gen:watch": "eleventy --config=.eleventy.js --watch",
    "lint:eslint": "eslint",
    "lint:lit-analyzer": "lit-analyzer",
    "serve": "wds --config=etc/web-dev-server/index.js --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "lit": "^3.1.4"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0-alpha.13",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@custom-elements-manifest/analyzer": "^0.10.2",
    "@eslint/js": "^9.5.0",
    "@stylistic/eslint-plugin-js": "^2.2.0",
    "@stylistic/eslint-plugin-ts": "^2.2.1",
    "@types/eslint__js": "^8.42.3",
    "@typescript-eslint/parser": "^8.0.0-alpha.30",
    "@web/dev-server": "^0.4.5",
    "del-cli": "^5.1.0",
    "eslint": "^9.5.0",
    "eslint-plugin-package-json": "^0.14.0",
    "globals": "^15.5.0",
    "jsonc-eslint-parser": "^2.4.0",
    "lit-analyzer": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.0.0-alpha.30"
  },
  "engines": {
    "node": ">=20.14.0"
  }
}
